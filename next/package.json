{
  "name": "@askdoppler/next",
  "version": "0.1.0",
  "description": "Next.js middleware for Doppler AI traffic monitoring.",
  "author": "AskDoppler <hello@askdoppler.com>",
  "keywords": [
    "doppler",
    "askdoppler",
    "nextjs",
    "ai-monitoring",
    "ai-seo",
    "traffic-analysis"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/askdoppler/doppler-js"
  },
  "license": "MIT",
  "type": "module",
