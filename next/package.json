  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@askdoppler/core": "^0.2.7"
  },
  "devDependencies": {
    "@types/node": "^22.15.20",
    "typescript": "^5.8.3"
  }
}
